---
layout: nil
---
<feed xmlns="http://www.w3.org/2005/Atom">

	<title type="text" xml:lang="en">{{ site.root_desc }}</title>
	<link type="application/atom+xml" href="http://blog.byjoemoon.com/feed/" rel="self"/>
 	<link type="text" href="http://blog.byjoemoon.com" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://byjoemoon.com</id>
	<author>
		<name>Joe Moon</name>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://blog.byjoemoon.com{{ post.url | remove: ".html" }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://blog.byjoemoon.com{{ post.id }}</id>
		<content type="html">{{ post.content }}</content>
	</entry>
	{% endfor %}
</feed>
